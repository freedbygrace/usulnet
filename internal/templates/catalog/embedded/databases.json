{
  "templates": [
    {
      "name": "PostgreSQL",
      "description": "Powerful open-source relational database",
      "image": "postgres:17-alpine",
      "ports": ["5432/tcp"],
      "volumes": [{"container": "/var/lib/postgresql/data"}],
      "env": [
        {"name": "POSTGRES_PASSWORD", "label": "Root password", "required": true, "type": "password"},
        {"name": "POSTGRES_DB", "label": "Database name", "default": "postgres"},
        {"name": "POSTGRES_USER", "label": "Username", "default": "postgres"}
      ]
    },
    {
      "name": "MySQL",
      "description": "Popular open-source relational database",
      "image": "mysql:8.4",
      "ports": ["3306/tcp"],
      "volumes": [{"container": "/var/lib/mysql"}],
      "env": [
        {"name": "MYSQL_ROOT_PASSWORD", "label": "Root password", "required": true, "type": "password"},
        {"name": "MYSQL_DATABASE", "label": "Database name", "default": "mydb"},
        {"name": "MYSQL_USER", "label": "Username"},
        {"name": "MYSQL_PASSWORD", "label": "User password", "type": "password"}
      ]
    },
    {
      "name": "MariaDB",
      "description": "Community-developed fork of MySQL",
      "image": "mariadb:11",
      "ports": ["3306/tcp"],
      "volumes": [{"container": "/var/lib/mysql"}],
      "env": [
        {"name": "MARIADB_ROOT_PASSWORD", "label": "Root password", "required": true, "type": "password"},
        {"name": "MARIADB_DATABASE", "label": "Database name", "default": "mydb"},
        {"name": "MARIADB_USER", "label": "Username"},
        {"name": "MARIADB_PASSWORD", "label": "User password", "type": "password"}
      ]
    },
    {
      "name": "MongoDB",
      "description": "Document-oriented NoSQL database",
      "image": "mongo:7",
      "ports": ["27017/tcp"],
      "volumes": [{"container": "/data/db"}],
      "env": [
        {"name": "MONGO_INITDB_ROOT_USERNAME", "label": "Root username", "default": "admin"},
        {"name": "MONGO_INITDB_ROOT_PASSWORD", "label": "Root password", "required": true, "type": "password"}
      ]
    },
    {
      "name": "Redis",
      "description": "In-memory data structure store and cache",
      "image": "redis:7-alpine",
      "ports": ["6379/tcp"],
      "volumes": [{"container": "/data"}],
      "command": "redis-server --appendonly yes"
    },
    {
      "name": "InfluxDB",
      "description": "Time-series database for metrics and events",
      "image": "influxdb:2",
      "ports": ["8086/tcp"],
      "volumes": [{"container": "/var/lib/influxdb2"}],
      "env": [
        {"name": "DOCKER_INFLUXDB_INIT_MODE", "label": "Init mode", "default": "setup"},
        {"name": "DOCKER_INFLUXDB_INIT_USERNAME", "label": "Username", "default": "admin"},
        {"name": "DOCKER_INFLUXDB_INIT_PASSWORD", "label": "Password", "required": true, "type": "password"},
        {"name": "DOCKER_INFLUXDB_INIT_ORG", "label": "Organization", "default": "myorg"},
        {"name": "DOCKER_INFLUXDB_INIT_BUCKET", "label": "Bucket", "default": "mybucket"}
      ]
    },
    {
      "name": "ClickHouse",
      "description": "Column-oriented database for real-time analytics",
      "image": "clickhouse/clickhouse-server:latest",
      "ports": ["8123/tcp", "9000/tcp"],
      "volumes": [{"container": "/var/lib/clickhouse"}, {"container": "/var/log/clickhouse-server"}],
      "env": [
        {"name": "CLICKHOUSE_USER", "label": "Username", "default": "default"},
        {"name": "CLICKHOUSE_PASSWORD", "label": "Password", "type": "password"}
      ]
    },
    {
      "name": "Valkey",
      "description": "Open-source Redis-compatible in-memory store",
      "image": "valkey/valkey:8-alpine",
      "ports": ["6379/tcp"],
      "volumes": [{"container": "/data"}],
      "command": "valkey-server --appendonly yes"
    }
  ]
}
