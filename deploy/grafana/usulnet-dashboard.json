{
  "annotations": {
    "list": []
  },
  "description": "usulnet Docker Management Platform - Overview Dashboard",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "Platform Overview",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "thresholds": { "steps": [{ "color": "green", "value": null }] } }
      },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "id": 1,
      "options": { "colorMode": "value", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Agents Connected",
      "type": "stat",
      "targets": [{ "expr": "usulnet_agents_connected_total", "legendFormat": "" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "thresholds": { "steps": [{ "color": "green", "value": null }] } }
      },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "id": 2,
      "options": { "colorMode": "value", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Running Containers",
      "type": "stat",
      "targets": [{ "expr": "usulnet_containers_by_state{state=\"running\"}", "legendFormat": "" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "orange", "value": 1 }, { "color": "red", "value": 10 }] } }
      },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "id": 3,
      "options": { "colorMode": "value", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Critical Vulnerabilities",
      "type": "stat",
      "targets": [{ "expr": "usulnet_vulnerabilities_total{severity=\"critical\"}", "legendFormat": "" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "thresholds": { "steps": [{ "color": "green", "value": null }] } }
      },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "id": 4,
      "options": { "colorMode": "value", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Backups (Success)",
      "type": "stat",
      "targets": [{ "expr": "usulnet_backups_total{status=\"success\"}", "legendFormat": "" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "red", "value": 1 }] } }
      },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "id": 5,
      "options": { "colorMode": "value", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Backups (Failed)",
      "type": "stat",
      "targets": [{ "expr": "usulnet_backups_total{status=\"failure\"}", "legendFormat": "" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "thresholds": { "steps": [{ "color": "green", "value": null }] } }
      },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "id": 6,
      "options": { "colorMode": "value", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "License Days Left",
      "type": "stat",
      "targets": [{ "expr": "usulnet_license_days_remaining", "legendFormat": "" }]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 101,
      "title": "Containers per Host",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" } }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "id": 10,
      "options": { "legend": { "displayMode": "table", "placement": "right" }, "tooltip": { "mode": "multi" } },
      "title": "Containers by State",
      "type": "timeseries",
      "targets": [{ "expr": "usulnet_containers_by_state", "legendFormat": "{{ state }}" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" } }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "id": 11,
      "options": { "legend": { "displayMode": "table", "placement": "right" }, "tooltip": { "mode": "multi" } },
      "title": "Containers per Host (Running)",
      "type": "timeseries",
      "targets": [{ "expr": "usulnet_containers_by_host{state=\"running\"}", "legendFormat": "{{ host }}" }]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "id": 102,
      "title": "Security",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" } }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "id": 20,
      "options": { "legend": { "displayMode": "table", "placement": "right" }, "tooltip": { "mode": "multi" } },
      "title": "Vulnerabilities by Severity",
      "type": "timeseries",
      "targets": [{ "expr": "usulnet_vulnerabilities_total", "legendFormat": "{{ severity }}" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" } }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "id": 21,
      "options": { "legend": { "displayMode": "table", "placement": "right" }, "tooltip": { "mode": "multi" } },
      "title": "Security Scans",
      "type": "timeseries",
      "targets": [
        { "expr": "rate(usulnet_security_scans_total{status=\"completed\"}[5m])", "legendFormat": "completed/s" },
        { "expr": "rate(usulnet_security_scans_total{status=\"failed\"}[5m])", "legendFormat": "failed/s" }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 },
      "id": 103,
      "title": "API Performance",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "reqps" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "id": 30,
      "options": { "legend": { "displayMode": "table", "placement": "right" }, "tooltip": { "mode": "multi" } },
      "title": "API Request Rate",
      "type": "timeseries",
      "targets": [
        { "expr": "sum(rate(usulnet_api_requests_total[5m])) by (method)", "legendFormat": "{{ method }}" }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "s" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
      "id": 31,
      "options": { "legend": { "displayMode": "table", "placement": "right" }, "tooltip": { "mode": "multi" } },
      "title": "Docker Operation Latency (avg)",
      "type": "timeseries",
      "targets": [
        { "expr": "rate(usulnet_docker_operation_duration_seconds_sum[5m]) / rate(usulnet_docker_operation_duration_seconds_count[5m])", "legendFormat": "{{ operation }}" }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 32 },
      "id": 104,
      "title": "Host Resources",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "percent", "max": 100 }
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 33 },
      "id": 40,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "title": "Host CPU Usage (%)",
      "type": "timeseries",
      "targets": [{ "expr": "usulnet_host_cpu_percent", "legendFormat": "{{ host }}" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "percent", "max": 100 }
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 33 },
      "id": 41,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "title": "Host Memory Usage (%)",
      "type": "timeseries",
      "targets": [{ "expr": "usulnet_host_memory_percent", "legendFormat": "{{ host }}" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "percent", "max": 100 }
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 33 },
      "id": 42,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "title": "Host Disk Usage (%)",
      "type": "timeseries",
      "targets": [{ "expr": "usulnet_host_disk_percent", "legendFormat": "{{ host }}" }]
    }
  ],
  "schemaVersion": 39,
  "tags": ["usulnet", "docker", "monitoring"],
  "templating": {
    "list": [
      {
        "current": {},
        "hide": 0,
        "name": "DS_PROMETHEUS",
        "options": [],
        "query": "prometheus",
        "type": "datasource"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "usulnet - Platform Overview",
  "uid": "usulnet-overview",
  "version": 1
}
